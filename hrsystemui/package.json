    "@angular/platform-server": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@angular/ssr": "^17.3.10",
    "@sentry/angular": "^9.0.0",
    "@sentry/cli": "^2.39.1",
    "@sentry/node": "^9.0.0",
    "express": "^5.0.0",
    "firebase": "^11.2.0",
